@article{zentelin_mobilisation_2015,
	title = {Mobilisation des proximités et gouvernance du cluster de {Paris}-{Saclay}:},
	volume = {Décembre},
	issn = {0180-7307},
	shorttitle = {Mobilisation des proximités et gouvernance du cluster de {Paris}-{Saclay}},
	url = {https://www.cairn.info/revue-d-economie-regionale-et-urbaine-2015-5-page-883.htm?ref=doi},
	doi = {10.3917/reru.155.0883},
	abstract = {La recherche actuelle s’intéresse aux potentialités de mobiliser les proximités pour faciliter la gouvernance d’un territoire, la gouvernance se nourrissant de conflits et de concertation. En théorie, le renouvellement de la conception de l’intérêt général devrait y contribuer. Cette note de recherche porte sur l’exemple emblématique du cluster de Paris-Saclay. D’abord sont présentés la longue histoire du cluster de Saclay, puis le compromis institutionnel exceptionnel mis en place pour remédier à une régulation locale inefficace, sans concertation ni appropriation/intégration du conflit. On observe que ce dispositif n’a dû son efficacité relative qu’aux inflexions que le directeur du nouvel établissement public a su donner aux orientations initiales, notamment en substituant à l’option concentration une approche plus paysagère. Enfin, la conclusion établit l’incapacité de l’État à mobiliser les proximités pour faciliter la gouvernance du territoire, et l’intérêt à poursuivre l’observation de ce projet dont l’avenir demeure dépendant des développements et des décisions à venir de nombreux acteurs, économiques, institutionnels, politiques et associatifs.},
	number = {5},
	urldate = {2024-06-04},
	journal = {Revue d’Économie Régionale \& Urbaine},
	author = {Zentelin, Jean-Louis},
	month = dec,
	year = {2015},
	pages = {883--898},
}

